{
  "name": "twt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "IS_PROD=true ng build --prod --aot",
    "build:prod": "npm run lang:down && IS_PROD=true ng build --prod --aot && npm run deploy",
    "gen:lang": "ng extract-i18n --format arb --ivy --out-file src/locale/messages.en-SK.json && node cli/lang_upload.js",
    "lang:up": "npm run gen:lang && localazy upload",
    "lang:down": "localazy download && node cli/lang_download.js",
    "dev:sk": "IS_PROD=false ng serve --configuration=sk --port 4203",
    "dev:en": "IS_PROD=false ng serve --configuration=en --port 4203",
    "deploy": "sshpass -p 'YPT-330piano' scp -r dist/twt/* root@hiroo.eu:/var/www/hiroo/ && sshpass -p 'YPT-330piano' scp -r dist/twt/en/assets/* root@hiroo.eu:/var/www/hiroo/assets && sshpass -p 'YPT-330piano' ssh root@hiroo.eu 'service nginx reload'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "glob": "^7.1.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/localize": "^11.1.1",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.2.3",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^11.0.0",
    "ngx-tailwind": "^2.0.0",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-scss": "^3.0.4",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
