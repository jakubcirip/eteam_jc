{
  "BasicResponse": {
    "type": "object",
    "required": ["success", "message"],
    "properties": {
      "success": {
        "type": "boolean"
      },
      "message": {
        "type": "string"
      },
      "data": {
        "type": "object",
        "properties": {
          "intId": {
            "type": "number"
          },
          "formId": {
            "type": "number"
          },
          "posId": {
            "type": "number"
          },
          "idsArr": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["id", "type"],
              "properties": {
                "id": {
                  "type": "number"
                },
                "type": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },

  "LoginGlobalResponse": {
    "type": "object",
    "required": ["success", "message", "domain", "key"],
    "properties": {
      "domain": {
        "type": "string"
      },
      "success": {
        "type": "boolean"
      },
      "message": {
        "type": "string"
      },
      "key": {
        "type": "string"
      }
    }
  }
}
